    <validationRules>
        <fullName>Close_Date_Alert</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( IsClosed )  &amp;&amp;  CloseDate  &lt;  TODAY()</errorConditionFormula>
        <errorMessage>The close date must be today or later (we wonâ€™t check this)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_Rate_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>OR(Discount_Rate__c &lt; 0, Discount_Rate__c &gt; 0.40)</errorConditionFormula>
        <errorMessage>The Discount Rate must not exceed 40%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test1</fullName>
        <active>false</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(StageName, &quot;Closed Won&quot;) , ISPICKVAL(StageName, &quot;Closed Lost&quot;)) ,(Amount&gt;50000),NOT(ISPICKVAL(Approval_Status__c ,&quot;Approved&quot;)))</errorConditionFormula>
        <errorMessage>All high-value opportunities must be approved for closure. Click the Request Close button</errorMessage>
    </validationRules>
